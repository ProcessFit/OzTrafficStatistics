get_sums <- function(df) {
popular <- data.frame(t(sapply(wt_names, function(x) table(wt[x]))))
popular$Package <- gsub("WorkToolsFrequency","",row.names(popular))
popular <- popular[,c(6,2,3,5,4)]
popular <- cbind(popular,data.frame(Score = 5*popular[,2]+3*popular[,3]+2*popular[,4]+popular[,5]))
popular$Package <- factor(popular$Package, levels = rev(arrange(popular,desc(Score))$Package))
popular <- arrange(popular,desc(Score))
popular
}
